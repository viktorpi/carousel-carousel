{
  "name": "carousel-carousel",
  "description": "JS component for rendering animated responsive slider with progress bullets",
  "version": "0.1.11",
  "keywords": [
    "slider",
    "carousel",
    "front-end",
    "responsive",
    "progress",
    "animation"
  ],
  "homepage": "https://github.com/yuliapi/carousel",
  "author": "Iuliia Pishculina (http://yuliapi.github.io/)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yuliapi/carousel.git"
  },
  "bugs": {
    "url": "https://github.com/yuliapi/carousel/issues"
  },
  "lisence": "MIT",
  "engines": {
    "node": ">=6"
  },
  "browserList": [],
  "scripts": {
    "css-compile": "node-sass --output-style expanded --sourec-map true --source-map-contents true scss/carousel.scss css/carousel.css",
    "js-compile": "babel --presets es2015 -d js/dist js/src",
    "js-minify": "uglifyjs --compress --mangle --output js/dist/carousel.min.js js/dist/carousel.js",
    "release": "npm-run-all css-* js-*",
    "prepublish": "npm-auto-version",
    "postpublish": "git push origin --tags"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "node-sass": "^4.7.2",
    "npm-auto-version": "^1.0.0",
    "npm-run-all": "^4.1.2",
    "uglify-js": "^3.3.9"
  },
  "dependencies": {}
}